;----------------------------------------------------------------
; NT 5.0 Edgeport
;----------------------------------------------------------------
[ClassInstall32.NTx86]
CopyFiles   =	ClassInstaller_CopyFiles.NTx86
AddReg      =   IonMultiPortSerialClass.NTx86.AddReg
 
[ClassInstall32.NTAMD64]
CopyFiles   =	ClassInstaller_CopyFiles.NTAMD64
AddReg      =   IonMultiPortSerialClass.NTAMD64.AddReg
 
;----------------------------------------------------------------
[Edgeport.Install.NTx86]
CopyFiles = Edgeport.NTx86.Files.Sys, Edgeport.NTx86.Files.Exe, Edgeport.NTx86.Files.Menu

[Edgeport.Install.NTAMD64]
CopyFiles = Edgeport.NTAMD64.Files.Sys, Edgeport.NTAMD64.Files.Exe, Edgeport.NTAMD64.Files.Menu

;----------------------------------------------------------------
[RapidPort.Install.NTx86]
CopyFiles = Edgeport.NTx86.Files.Sys, Edgeport.NTx86.Files.Exe, Rapidport.NTx86.Files.Exe, Rapidport.NTx86.Files.Menu

[RapidPort.Install.NTAMD64]
CopyFiles = Edgeport.NTAMD64.Files.Sys, Edgeport.NTAMD64.Files.Exe, Rapidport.NTAMD64.Files.Exe, Rapidport.NTAMD64.Files.Menu

;----------------------------------------------------------------
[Edgeport.NTx86.Files.Sys]
edgeser.sys
ionenum.sys

[Edgeport.NTAMD64.Files.Sys]
edgeser64.sys
ionenum64.sys

;----------------------------------------------------------------
[ClassInstaller_CopyFiles.NTx86]
ImpsCoin.dll

[ClassInstaller_CopyFiles.NTAMD64]
ImpsCoin64.dll

;----------------------------------------------------------------
[CoInstaller_CopyFiles.NTx86]
EdgeCoIn.dll

[CoInstaller_CopyFiles.NTAMD64]
EdgeCoIn64.dll

;----------------------------------------------------------------
[Edgeport.NTx86.Files.Exe]
edgeport.exe

[Edgeport.NTAMD64.Files.Exe]
edgeport64.exe

;----------------------------------------------------------------
[Rapidport.NTx86.Files.Exe]
Ionflash.exe

[Rapidport.NTAMD64.Files.Exe]
Ionflash.exe

;----------------------------------------------------------------
[Edgeport.NTx86.Files.Menu]
"Edgeport Configuration Utility.lnk",Edgeport.lnk,,

[Edgeport.NTAMD64.Files.Menu]
"Edgeport Configuration Utility.lnk",Edgeport64.lnk,,

;----------------------------------------------------------------
[Rapidport.NTx86.Files.Menu]
"Edgeport Configuration Utility.lnk",Edgeport.lnk,,
"Rapidport Firmware Update Utility.lnk",Ionflash.lnk,,

[Rapidport.NTAMD64.Files.Menu]
"Edgeport Configuration Utility.lnk",Edgeport64.lnk,,
"Rapidport Firmware Update Utility.lnk",Ionflash.lnk,,

;----------------------------------------------------------------
[Edgeport.Install.NTx86.Services]
AddService = EdgeSer, 0x00000002, Edgeport.NTx86.AddService, Edgeport_EventLog_Inst.NTx86

[Edgeport.Install.NTAMD64.Services]
AddService = EdgeSer, 0x00000002, Edgeport.NTAMD64.AddService, Edgeport_EventLog_Inst.NTAMD64

;----------------------------------------------------------------
[RapidPort.Install.NTx86.Services]
AddService = EdgeSer, 0x00000002, Edgeport.NTx86.AddService, Edgeport_EventLog_Inst.NTx86

[RapidPort.Install.NTAMD64.Services]
AddService = EdgeSer, 0x00000002, Edgeport.NTAMD64.AddService, Edgeport_EventLog_Inst.NTAMD64

;----------------------------------------------------------------
[Edgeport.NTx86.AddService]
DisplayName    = %EpDriverName%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\edgeser.sys   ; 12 => nt/system32/drivers

[Edgeport.NTAMD64.AddService]
DisplayName    = %EpDriverName%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\edgeser64.sys ; 12 => nt/system32/drivers

;----------------------------------------------------------------
[Edgeport.Install.NTx86.CoInstallers]
CopyFiles = CoInstaller_CopyFiles.NTx86
AddReg=Edgeport.NTx86.CoInstallers_AddReg

[Edgeport.Install.NTAMD64.CoInstallers]
CopyFiles = CoInstaller_CopyFiles.NTAMD64
AddReg=Edgeport.NTAMD64.CoInstallers_AddReg

;----------------------------------------------------------------
[Edgeport.NTx86.CoInstallers_AddReg]
HKR,,CoInstallers32,0x00010000,"EdgeCoIn.dll,EdgeCoInstaller"
HKLM,System\CurrentControlSet\Services\EdgeSer,Family,0x00000000,%EpFamilyName%

[Edgeport.NTAMD64.CoInstallers_AddReg]
HKR,,CoInstallers32,0x00010000, "EdgeCoIn64.dll,EdgeCoInstaller"
HKLM,System\CurrentControlSet\Services\EdgeSer,Family,0x00000000,%EpFamilyName%

;----------------------------------------------------------------
[IonMultiPortSerialClass.NTx86.AddReg]
HKR,,Installer32,,"ImpsCoin.dll,IonMpsClassInstaller"
HKR,,Icon,,"0"

[IonMultiPortSerialClass.NTAMD64.AddReg]
HKR,,Installer32,,"ImpsCoin64.dll,IonMpsClassInstaller"
HKR,,Icon,,"0"

;----------------------------------------------------------------
[Edgeport_EventLog_Inst.NTx86]
AddReg = Edgeport_EventLog_AddReg.NTx86

[Edgeport_EventLog_Inst.NTAMD64]
AddReg = Edgeport_EventLog_AddReg.NTAMD64

;----------------------------------------------------------------
[Edgeport_EventLog_AddReg.NTx86]
HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\edgeser.sys"
HKR,,TypesSupported,0x00010001,7

[Edgeport_EventLog_AddReg.NTAMD64]
HKR,,EventMessageFile,0x00020000,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\edgeser64.sys"
HKR,,TypesSupported,0x00010001,7


