[Version]
Signature   = "$CHICAGO$"
Class       = IonMultiPortSerial
ClassGuid   = {1A48FDD5-9D23-4889-93AA-78967DB78D30}
Provider    = %MfgName%
CatalogFile = edgeport.cat
DriverVer=05/05/2010, 5.30.02.0


;----------------------------------------------------------------
; This section lists the diskette names. Format is:
;	diskid = %strkey% | ["]disk-description["][[,tagfile][,unused,path]]
;----------------------------------------------------------------
[SourceDisksNames]
1 = %EpDiskName%,edgeport.cab,,


;----------------------------------------------------------------
; This section gives location of each source file. Format is:
;	filename = diskid[,[ subdir][, size]]
;----------------------------------------------------------------
[SourceDisksFiles]
edgeser.sys = 1,,
edgeser64.sys = 1,,
ionenum.sys = 1,,
ionenum64.sys = 1,,
EdgeCoIn.dll = 1,,
EdgeCoIn64.dll = 1,,
ImpsCoin.dll = 1,,
ImpsCoin64.dll = 1,,
edgeport.exe = 1,,
edgeport64.exe = 1,,
Edgeport.lnk = 1,,
Edgeport64.lnk = 1,,
Ionflash.exe = 1,,
Ionflash.lnk = 1,,


;----------------------------------------------------------------
; This section gives location of where to put each destination file.
; Format is:
;	DefaultDestDir = dirid[,subdir]] 
;	file-list-section = dirid[,subdir]]... 
;----------------------------------------------------------------
[DestinationDirs]
Edgeport.NTx86.Files.Sys = 12			; System32\Drivers
Edgeport.NTAMD64.Files.Sys = 12			; System32\Drivers

Edgeport.NTx86.Files.Exe = 11 			; System32
Edgeport.NTAMD64.Files.Exe = 11 		; System32

Rapidport.NTx86.Files.Exe = 11 			; System32
Rapidport.NTAMD64.Files.Exe = 11 		; System32

CoInstaller_CopyFiles.NTx86 = 11 		; System32
CoInstaller_CopyFiles.NTAMD64 = 11 		; System32

Edgeport.NTx86.Files.Menu = 16407,\Digi USB
Edgeport.NTAMD64.Files.Menu = 16407,\Digi USB

Rapidport.NTx86.Files.Menu = 16407,\Digi USB
Rapidport.NTAMD64.Files.Menu = 16407,\Digi USB


;----------------------------------------------------------------
; This section gives flags for this INF file. Format is:
;	ExcludeFromSelect[.nt[xxx]] = [*] | [hw-id[,hw-id]...]
;	CopyFilesOnly = hw-id[,hw-id]...
;----------------------------------------------------------------
[ControlFlags]
ExcludeFromSelect = *		; Hide all devices here from manual install


;----------------------------------------------------------------
; This section lists the manufacturer of devices installable with
; this INF file, and gives the name of the [Models] section for each
; manufacturer. The format is:
;	manufacturer-name...	|
;	%strkey% = models-section-name...
